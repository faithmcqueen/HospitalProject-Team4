@model HospitalProject_Team4.Models.Feedback
@{
    ViewBag.Title = "ShowFeedback";
}

<h2>Show Feedback</h2>

<div class="navbar">
    <a href="/Feedback/ListOfFeedbacks" class="btn btn-default">Back to List</a>
    <a href="/Feedback/UpdateFeedback/@Model.feedback_id" class="btn btn-default">Update</a>
    <a href="/Feedback/DeleteFeedback/@Model.feedback_id" class="btn btn-danger">Delete</a>
</div>
<div>
    <div>First Name : @Model.first_name</div><br/>
    <div>Last Name : @Model.last_name</div><br />
    <div>Subject of Feedback : @Model.feedback_subject</div><br />
    <div>Comment : @Model.feedback_message</div><br />
    <div>Date : @Model.feedback_date</div>
</div>