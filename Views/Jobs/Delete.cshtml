@model HospitalProject_Team4.Models.JobPosting
@{
    ViewBag.Title = "Delete";
}

<h2>Are you sure you want to delete @Model.JobTitle?</h2>
<div class="navbar">
    <a class="btn btn-secondary" href="/Jobs/Admin/Show/@Model.PostingID">Cancel</a>
</div>
<form method="POST" action="/Jobs/Admin/Delete/@Model.PostingID">
    <div><input class="btn btn-danger" type="submit" value="Confirm" /></div>
</form>


